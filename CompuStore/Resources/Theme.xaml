<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:conv="clr-namespace:CompuStore.Infrastructure.Converters;assembly=CompuStore.Infrastructure">
    <BitmapImage x:Key="Logo" UriSource="../Images/Logo.png"></BitmapImage>
    <Style x:Key="SummeryLabel" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="Times New Roman"/>
        <Setter Property="FontSize" Value="20"/>
        <Setter Property="Foreground" Value="{DynamicResource IdealForeGroundColorBrush}"/>
    </Style>
    <Style x:Key="SummeryValue" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="Times New Roman"/>
        <Setter Property="FontSize" Value="18"/>
        <Setter Property="FontWeight" Value="DemiBold"/>        
    </Style>
    <conv:PositiveNegativeColorConverter x:Key="PositiveNegativeConverter"/>
    <conv:VisibleHiddenBoolConverter x:Key="HiddenVisibleConverter"/>
</ResourceDictionary>